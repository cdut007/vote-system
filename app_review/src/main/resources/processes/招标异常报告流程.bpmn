  <process id="evaluation_exception_review" name="招标异常报告核流程" isExecutable="true">
    <startEvent id="startevent1" name="招标异常报告审核开始" activiti:initiator="applyUserId"></startEvent>
    <userTask id="usertask1" name="代理机构审核" activiti:assignee="${projectManagerId}" activiti:formKey="recruit/node_10">
      <documentation>${exceptionTitle} 招标异常报告-项目经理审核</documentation>
      <extensionElements>
        <activiti:formProperty id="exceptionId" name="招标异常报告ID" type="string" expression="${execution.processBusinessKey}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="pass" name="是否通过" type="boolean" default="true" required="true"></activiti:formProperty>
        <activiti:formProperty id="nopassReason" name="不通过原因" type="string"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="scripttask1"></sequenceFlow>
    <userTask id="usertask2" name="招标人审核" activiti:assignee="${ownerId}" activiti:formKey="recruit/node_11">
      <documentation>${exceptionTitle} 招标异常报告-招标人审核</documentation>
      <extensionElements>
        <activiti:formProperty id="exceptionId" name="招标异常报告ID" type="string" expression="${execution.processBusinessKey}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="pass" name="是否通过" type="boolean" variable="pass2" default="true" required="true"></activiti:formProperty>
        <activiti:formProperty id="nopassReason" name="不通过原因" type="string" variable="nopassReason"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow2" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow5" name="审核通过" sourceRef="exclusivegateway1" targetRef="usertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${pass&& not empty ownerId}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="usertask2" targetRef="exclusivegateway2"></sequenceFlow>
    <endEvent id="endevent2" name="审核不通过"></endEvent>
    <sequenceFlow id="flow10" name="审核不通过" sourceRef="exclusivegateway1" targetRef="endevent2">
      <extensionElements>
        <activiti:executionListener event="take" expression="${tenderExceptionService.updateTenderException(execution.processBusinessKey,'2',nopassReason)}"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!pass}]]></conditionExpression>
    </sequenceFlow>
    <scriptTask id="scripttask1" name="变量初始化" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>def tbe=baseService.get("com.mhb.evaluation.entity.TBEvaluationException",execution.processBusinessKey);
def projectInstance=baseService.get("com.mhb.project.entity.TBProjectInstance",tbe.projectInstanceId);
execution.setVariable("exceptionTitle",tbe.getExceptionTitle());
execution.setVariable("projectManagerId",projectInstance.getProjectManagerId());
execution.setVariable("ownerId",projectInstance.getOwnerId());
execution.setVariable("exceptionId",execution.processBusinessKey);
</script>
    </scriptTask>
    <sequenceFlow id="flow13" sourceRef="scripttask1" targetRef="usertask1"></sequenceFlow>
    <sequenceFlow id="flow14" sourceRef="exclusivegateway1" targetRef="servicetask1">
      <extensionElements>
        <activiti:executionListener event="take" expression="${tenderExceptionService.updateTenderException(execution.processBusinessKey,'9')}"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${pass&& empty ownerId}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow17" name="审核通过" sourceRef="exclusivegateway2" targetRef="servicetask1">
      <extensionElements>
        <activiti:executionListener event="take" expression="${tenderExceptionService.updateTenderException(execution.processBusinessKey,'9')}"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${pass2}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow18" name="审核不通过" sourceRef="exclusivegateway2" targetRef="endevent2">
      <extensionElements>
        <activiti:executionListener event="take" expression="${tenderExceptionService.updateTenderException(execution.processBusinessKey,'3',nopassReason)}"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!pass2}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent3" name="End"></endEvent>
    <serviceTask id="servicetask1" name="更新招标异常报告状态" activiti:expression="${tenderExceptionService.updateTenderException(execution.processBusinessKey,'9')}"></serviceTask>
    <sequenceFlow id="flow19" sourceRef="servicetask1" targetRef="endevent3"></sequenceFlow>
  </process>
