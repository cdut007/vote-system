    </sequenceFlow>
    <serviceTask id="servicetask2" name="更新标段状态" activiti:expression="${candidatePublicityService.updateCandidatePublicityDataInProjectItem(execution.processBusinessKey)}"></serviceTask>
    <sequenceFlow id="flow27" sourceRef="servicetask2" targetRef="endevent1"></sequenceFlow>
