  <process id="candidate_publicity_review" name="中标候选人审核流程" isExecutable="true">
    <extensionElements>
      <activiti:executionListener event="end" expression="${publicityReviewEndListener.execute(execution)}"></activiti:executionListener>
    </extensionElements>
    <startEvent id="startevent1" name="中标候选人公示审核" activiti:initiator="applyUserId"></startEvent>
    <userTask id="usertask1" name="项目经理审核" activiti:assignee="${projectManagerId}" activiti:formKey="notice/publicity_review_pm">
      <documentation>${projectInstanceName} 中标候选人公示审核-项目经理审核</documentation>
      <extensionElements>
        <activiti:formProperty id="pass" name="审核通过" type="boolean" required="true"></activiti:formProperty>
        <activiti:formProperty id="shyj" name="审核意见" type="string"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="scripttask1"></sequenceFlow>
    <userTask id="usertask2" name="招标人审核" activiti:assignee="${ownerId}" activiti:formKey="notice/publicity_review">
      <documentation>${projectInstanceName} 中标候选人公示审核-招标人审核</documentation>
      <extensionElements>
        <activiti:formProperty id="pass" name="审核通过" type="boolean" required="true"></activiti:formProperty>
        <activiti:formProperty id="shyj" name="审核意见" type="string"></activiti:formProperty>
