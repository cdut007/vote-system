    <scriptTask id="scripttask1" name="初始化变量" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>def candidatePublicity=baseService.get("com.mhb.notice.entity.TBCandidatePublicity",execution.processBusinessKey);
def projectInstance=baseService.get("com.mhb.project.entity.TBProjectInstance",candidatePublicity.projectInstanceId);
execution.setVariable("projectInstanceId",candidatePublicity.getProjectInstanceId());
execution.setVariable("projectInstanceName",candidatePublicity.getProjectInstanceName());
/*execution.setVariable("projectItemId",candidatePublicity.getProjectItemId());*/
/*execution.setVariable("projectItemName",candidatePublicity.getProjectItemName());*/
execution.setVariable("projectManagerId",projectInstance.getProjectManagerId());
execution.setVariable("ownerId",projectInstance.getOwnerId());
execution.setVariable("dlrzj",userService.getPrincipalIdByOrganId(projectInstance.getOrganId()));
def sdf=new java.text.SimpleDateFormat("yyyy-MM-dd hh:mm:ss");
def dateStr=sdf.format(candidatePublicity.getGskssj());
out:println "中标公告发布时间:"+dateStr;
def publishTime=sdf.parse(dateStr);
execution.setVariable("publishTime",candidatePublicity.getGskssj());</script>
    </scriptTask>
    <sequenceFlow id="flow23" sourceRef="scripttask1" targetRef="usertask1"></sequenceFlow>
    <userTask id="usertask3" name="交易平台审核" activiti:candidateGroups="1ae9a74b-784e-41e4-9c04-7d7d7585cdda" activiti:formKey="notice/platform_publicity_review">
      <documentation>${projectInstanceName} 中标候选人公示审核-交易平台审核</documentation>
