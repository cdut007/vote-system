<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="evaluation_clarify" name="评标答疑澄清" isExecutable="true">
    <startEvent id="startevent1" name="发起澄清" activiti:initiator="applyUserId"></startEvent>
    <userTask id="usertask1" name="专家澄清确认" activiti:assignee="${userId}" activiti:formKey="evaluation/clarify/evaluationClarifyVote">
      <documentation>评标专家 ${userName} 对 投标单位 ${organBName} 发起了一个澄清，请确认。</documentation>
      <extensionElements>
        <activiti:formProperty id="agree" name="是否同意" type="string" default="1" required="true"></activiti:formProperty>
        <activiti:taskListener event="complete" expression="${evaluationClarifyService.createEvaluationClarifyVote(execution.processBusinessKey,agree)}"></activiti:taskListener>
      </extensionElements>
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="${expertIdListWithoutSelf}" activiti:elementVariable="userId"></multiInstanceLoopCharacteristics>
    </userTask>
    <userTask id="usertask2" name="投标人回复" activiti:assignee="${bUserId}" activiti:formKey="evaluation/clarify/evaluationClarifyAnswer">
      <documentation>评标专家组 需要您 对 ${projectInstanceName} ${projectItemName} 投标文件进行澄清。</documentation>
      <extensionElements>
        <activiti:formProperty id="answer" name="澄清回答" type="string" required="true"></activiti:formProperty>
        <activiti:taskListener event="complete" expression="${evaluationClarifyService.answerEvaluationClarify(execution.processBusinessKey,answer)}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow2" sourceRef="usertask1" targetRef="servicetask1"></sequenceFlow>
    <userTask id="usertask3" name="专家确认回复" activiti:assignee="${userId}" activiti:formKey="evaluation/clarify/evaluationClarifyReceive">
      <documentation>${organBName} 已经对 ${projectInstanceName} ${projectItemName} 投标文件进行澄清，请确认。</documentation>
      <extensionElements>
      </extensionElements>
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="${expertIdList}" activiti:elementVariable="userId"></multiInstanceLoopCharacteristics>
    </userTask>
    <sequenceFlow id="flow3" sourceRef="usertask2" targetRef="usertask3"></sequenceFlow>
    <endEvent id="endevent1" name="澄清结束"></endEvent>
    <sequenceFlow id="flow4" sourceRef="usertask3" targetRef="endevent1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <endEvent id="endevent2" name="终止澄清"></endEvent>
    <sequenceFlow id="flow5" sourceRef="exclusivegateway1" targetRef="endevent2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!passed}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="exclusivegateway1" targetRef="usertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${passed}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask1" name="计算投标结果" activiti:expression="${evaluationClarifyService.isEvaluationClarifyVotePass(execution.processBusinessKey)}" activiti:resultVariableName="passed"></serviceTask>
    <sequenceFlow id="flow7" sourceRef="servicetask1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow8" sourceRef="startevent1" targetRef="scripttask1"></sequenceFlow>
    <scriptTask id="scripttask1" name="变量初始化" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>def evaluationClarify=baseService.get("com.mhb.evaluation.entity.TBEvaluationClarify",execution.processBusinessKey);
execution.setVariable("userName",evaluationClarify.userName);
execution.setVariable("projectInstanceName",evaluationClarify.projectInstanceName);
execution.setVariable("projectItemName",evaluationClarify.projectItemName);
execution.setVariable("organBName",evaluationClarify.organBName);</script>
    </scriptTask>
    <sequenceFlow id="flow9" sourceRef="scripttask1" targetRef="usertask1"></sequenceFlow>
  </process>
  <bpmndi:BPMNDiagram id="BPMNDiagram_evaluation_clarify">
    <bpmndi:BPMNPlane bpmnElement="evaluation_clarify" id="BPMNPlane_evaluation_clarify">
      <bpmndi:BPMNShape bpmnElement="startevent1" id="BPMNShape_startevent1">
        <omgdc:Bounds height="35.0" width="35.0" x="30.0" y="220.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="usertask1" id="BPMNShape_usertask1">
        <omgdc:Bounds height="55.0" width="105.0" x="245.0" y="210.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="usertask2" id="BPMNShape_usertask2">
        <omgdc:Bounds height="55.0" width="105.0" x="620.0" y="210.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="usertask3" id="BPMNShape_usertask3">
        <omgdc:Bounds height="55.0" width="105.0" x="800.0" y="210.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="endevent1" id="BPMNShape_endevent1">
        <omgdc:Bounds height="35.0" width="35.0" x="1000.0" y="220.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="exclusivegateway1" id="BPMNShape_exclusivegateway1">
        <omgdc:Bounds height="40.0" width="40.0" x="531.0" y="217.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="endevent2" id="BPMNShape_endevent2">
        <omgdc:Bounds height="35.0" width="35.0" x="534.0" y="310.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="servicetask1" id="BPMNShape_servicetask1">
        <omgdc:Bounds height="55.0" width="105.0" x="390.0" y="210.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="scripttask1" id="BPMNShape_scripttask1">
        <omgdc:Bounds height="55.0" width="105.0" x="100.0" y="210.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge bpmnElement="flow2" id="BPMNEdge_flow2">
        <omgdi:waypoint x="350.0" y="237.0"></omgdi:waypoint>
        <omgdi:waypoint x="390.0" y="237.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow3" id="BPMNEdge_flow3">
        <omgdi:waypoint x="725.0" y="237.0"></omgdi:waypoint>
        <omgdi:waypoint x="800.0" y="237.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow4" id="BPMNEdge_flow4">
        <omgdi:waypoint x="905.0" y="237.0"></omgdi:waypoint>
        <omgdi:waypoint x="1000.0" y="237.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow5" id="BPMNEdge_flow5">
        <omgdi:waypoint x="551.0" y="257.0"></omgdi:waypoint>
        <omgdi:waypoint x="551.0" y="310.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow6" id="BPMNEdge_flow6">
        <omgdi:waypoint x="571.0" y="237.0"></omgdi:waypoint>
        <omgdi:waypoint x="620.0" y="237.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow7" id="BPMNEdge_flow7">
        <omgdi:waypoint x="495.0" y="237.0"></omgdi:waypoint>
        <omgdi:waypoint x="531.0" y="237.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow8" id="BPMNEdge_flow8">
        <omgdi:waypoint x="65.0" y="237.0"></omgdi:waypoint>
        <omgdi:waypoint x="100.0" y="237.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow9" id="BPMNEdge_flow9">
        <omgdi:waypoint x="205.0" y="237.0"></omgdi:waypoint>
        <omgdi:waypoint x="245.0" y="237.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</definitions>