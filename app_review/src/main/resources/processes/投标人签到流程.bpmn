  <signal id="guardianSignSignal" name="guardianSignSignal"></signal>
  <signal id="auditTenderSignSignal" name="auditTenderSignSignal"></signal>
  <process id="tender_sign_work_flow" name="投标人签到流程" isExecutable="true">
    <userTask id="usertask19" name="投标人签到" activiti:assignee="${userId}" activiti:dueDate="${bidOpenDate}" activiti:formKey="redirect:/bidOpen/bidOpenManager?expertApplyId=${execution.processBusinessKey}">
      <documentation>${projectInstanceName} 即将开标，请您在开标时间到达前签到</documentation>
      <extensionElements>
        <activiti:taskListener event="complete" expression="${signService.bidderSign(execution.processBusinessKey)}"></activiti:taskListener>
      </extensionElements>
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="${tenderUserIdSign}" activiti:elementVariable="userId"></multiInstanceLoopCharacteristics>
    </userTask>
    <startEvent id="startevent11" name="等待开标"></startEvent>
    <scriptTask id="scripttask1" name="变量初始化" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>def expertApplyId=execution.getVariable("expertApplyId");
def expertApply=baseService.get("com.mhb.expert.entity.TBExpertApply",expertApplyId);
def map = ["expertApplyId":expertApplyId];
baseService.executeByHQL("delete TBSign where expertApplyId=:expertApplyId and userType='3'",map);
/*runtimeService.updateBusinessKey(execution.getProcessInstanceId(), expertApplyId);*/
/*cacheManager.getCache("expertApplyCache").put(new net.sf.ehcache.Element(execution.processBusinessKey,expertApply));*/
/*chatManagerService.sendSystemMessage(dlrzj0,freeMarkerBean.getFileTemplateResult("message/bidOpenMessage.ftl",execution.variables));*/
def tenderUserIdSign=userService.retrieveTenderUserIdSign(execution.processBusinessKey);
execution.setVariable('tenderUserIdSign',tenderUserIdSign);/*获取有效投标人*/
      	
      </script>
    </scriptTask>
    <sequenceFlow id="flow238" sourceRef="startevent11" targetRef="scripttask1"></sequenceFlow>
    <endEvent id="endevent21" name="End"></endEvent>
    <sequenceFlow id="flow243" name="审核投标人签到" sourceRef="parallelgateway2" targetRef="signalintermediatecatchevent2">
      <extensionElements>
        <activiti:executionListener event="take" expression="${guardianService.strartGuardianWorkflow(execution.processBusinessKey,'auditTenderSign')}"></activiti:executionListener>
      </extensionElements>
    </sequenceFlow>
    <sequenceFlow id="flow254" name="监标人签到" sourceRef="parallelgateway1" targetRef="signalintermediatecatchevent1">
      <extensionElements>
        <activiti:executionListener event="take" expression="${guardianService.strartGuardianWorkflow(execution.processBusinessKey,&quot;sign&quot;)}"></activiti:executionListener>
      </extensionElements>
    </sequenceFlow>
    <intermediateCatchEvent id="signalintermediatecatchevent1" name="SignalCatchEvent">
      <signalEventDefinition signalRef="guardianSignSignal"></signalEventDefinition>
    </intermediateCatchEvent>
    <sequenceFlow id="flow255" sourceRef="parallelgateway1" targetRef="usertask19"></sequenceFlow>
    <intermediateCatchEvent id="signalintermediatecatchevent2" name="SignalCatchEvent">
      <signalEventDefinition signalRef="auditTenderSignSignal"></signalEventDefinition>
    </intermediateCatchEvent>
    <sequenceFlow id="flow256" sourceRef="signalintermediatecatchevent2" targetRef="endevent21"></sequenceFlow>
    <parallelGateway id="parallelgateway1" name="Parallel Gateway"></parallelGateway>
    <sequenceFlow id="flow257" sourceRef="scripttask1" targetRef="parallelgateway1"></sequenceFlow>
    <parallelGateway id="parallelgateway2" name="Parallel Gateway"></parallelGateway>
    <sequenceFlow id="flow258" sourceRef="usertask19" targetRef="parallelgateway2"></sequenceFlow>
    <sequenceFlow id="flow259" sourceRef="signalintermediatecatchevent1" targetRef="parallelgateway2"></sequenceFlow>
  </process>
