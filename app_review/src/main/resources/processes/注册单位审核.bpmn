  <process id="regist" name="注册单位审核" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="usertask1" name="注册单位审核" activiti:candidateGroups="1ae9a74b-784e-41e4-9c04-7d7d7585cdda" activiti:formKey="base/organ/organReview">
      <documentation>${organName} 提交企业信息，请审核。</documentation>
      <extensionElements>
        <activiti:formProperty id="pass" name="是否通过" type="boolean" required="true"></activiti:formProperty>
        <activiti:formProperty id="nopassReason" name="不通过原因" type="string"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow2" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow3" sourceRef="exclusivegateway1" targetRef="scripttask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!pass}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent3" name="End"></endEvent>
    <sequenceFlow id="flow5" sourceRef="exclusivegateway1" targetRef="scripttask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${pass}]]></conditionExpression>
    </sequenceFlow>
    <scriptTask id="scripttask1" name="更新审核失败" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>def map=["organId":execution.processBusinessKey,"nopassReason":execution.getVariable("nopassReason")];
baseService.executeByHQL("update TBOrgan set isAudits='-1',backReason=:nopassReason  where id=:organId",map);</script>
    </scriptTask>
    <sequenceFlow id="flow6" sourceRef="scripttask1" targetRef="endevent1"></sequenceFlow>
    <scriptTask id="scripttask2" name="更新审核成功" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>def map=["organId":execution.processBusinessKey];
baseService.executeByHQL("update TBOrgan set isAudits='2',description=description_tmp,businessScope=businessScope_tmp where id=:organId",map);</script>
    </scriptTask>
    <sequenceFlow id="flow7" sourceRef="scripttask2" targetRef="endevent3"></sequenceFlow>
    <sequenceFlow id="flow8" sourceRef="startevent1" targetRef="scripttask3"></sequenceFlow>
    <scriptTask id="scripttask3" name="变量初始化" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>def organ=baseService.get("com.mhb.base.entity.TBOrgan",execution.processBusinessKey);
execution.setVariable('organName',organ.ztmc00);</script>
    </scriptTask>
    <sequenceFlow id="flow9" sourceRef="scripttask3" targetRef="usertask1"></sequenceFlow>
  </process>
