  <process id="invitation_file_workflow" name="投标邀请书" isExecutable="true">
    <startEvent id="startevent1" name="投标邀请开始"></startEvent>
    <userTask id="usertask1" name="项目经理审核" activiti:assignee="${projectManagerId}" activiti:formKey="document/approveInvitationFile">
      <documentation>${projectInstanceName} - ${projectItemName} 投标邀请书审核</documentation>
        <extensionElements>
        <activiti:formProperty id="pass" name="是否通过" type="boolean" default="true" required="true"></activiti:formProperty>
        <activiti:formProperty id="nopassReason" name="审核意见" type="string"></activiti:formProperty>     
      </extensionElements>
    </userTask>
    <endEvent id="endevent1" name="结束"></endEvent>
    <sequenceFlow id="flow3" sourceRef="startevent1" targetRef="usertask1"></sequenceFlow>
    <userTask id="usertask2" name="招标人签章" activiti:assignee="${zbrid}" activiti:formKey="document/signInvitationFileT">
      <documentation>${projectInstanceName} - ${projectItemName} 投标邀请书签章</documentation> 
    </userTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <userTask id="usertask3" name="代理机构签章" activiti:assignee="${dlrzjjid}" activiti:formKey="document/signInvitationFileA">
      <documentation>${projectInstanceName} - ${projectItemName} 投标邀请书签章</documentation>
    </userTask>
    <sequenceFlow id="flow7" sourceRef="exclusivegateway1" targetRef="usertask3">
      
       <extensionElements>
       	 	<activiti:executionListener event="take" expression="${invitationFileService.updateReviewStatus(execution.processBusinessKey,'5',nopassReason)}"></activiti:executionListener>
      	</extensionElements>
      	<conditionExpression xsi:type="tFormalExpression"><![CDATA[${pass}]]></conditionExpression>
    </sequenceFlow>
        <sequenceFlow id="flow14" sourceRef="exclusivegateway1" targetRef="endevent2">
    	 <extensionElements>
       	 	<activiti:executionListener event="take" expression="${invitationFileService.updateReviewStatus(execution.processBusinessKey,'19',nopassReason)}"></activiti:executionListener>
      	</extensionElements>
        <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!pass}]]></conditionExpression>
      	
    </sequenceFlow>
    <userTask id="usertask4" name="平台审核" activiti:candidateGroups="1ae9a74b-784e-41e4-9c04-7d7d7585cdda"  activiti:formKey="document/approveInvitationFileAdmin">
      <documentation>${projectInstanceName} - ${projectItemName} 投标邀请书平台审核</documentation>
      <extensionElements>
        <activiti:formProperty id="platformPrice" name="审核意见" type="double"></activiti:formProperty> 
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow10" sourceRef="usertask4" targetRef="endevent1">
    	 <extensionElements>
       	 	<activiti:executionListener event="take" expression="${invitationFileService.updateReviewStatusByAdmin(execution.processBusinessKey,'9',platformPrice)}"></activiti:executionListener>
      	</extensionElements>
    </sequenceFlow>
    <sequenceFlow id="flow11" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow12" sourceRef="usertask3" targetRef="usertask2">
      	<extensionElements>
       	 	<activiti:executionListener event="take" expression="${invitationFileService.updateReviewStatus(execution.processBusinessKey,'6','')}"></activiti:executionListener>
      	</extensionElements>
    </sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="usertask2" targetRef="usertask4">
    	<extensionElements>
       	 	<activiti:executionListener event="take" expression="${invitationFileService.updateReviewStatus(execution.processBusinessKey,'7','')}"></activiti:executionListener>
      	</extensionElements>
    </sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>

  </process>
