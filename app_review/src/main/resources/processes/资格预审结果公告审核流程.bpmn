  <process id="qualification_notice_review" name="资格预审结果公告审核流程" isExecutable="true">
    <startEvent id="startevent1" name="资格预审结果公示审核" activiti:initiator="applyUserId"></startEvent>
    <userTask id="usertask1" name="项目经理审核" activiti:assignee="${projectManagerId}" activiti:formKey="prequalification/noticeReview">
      <documentation>资格预审结果公告审核-代理机构负责人审核</documentation>
      <extensionElements>
        <activiti:formProperty id="pass" name="审核通过" type="boolean" required="true"></activiti:formProperty>
        <activiti:formProperty id="shyj" name="审核意见" type="string"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <userTask id="usertask2" name="招标人审核" activiti:assignee="${ownerId}" activiti:formKey="prequalification/noticeReview">
      <documentation>资格预审结果公告审核-招标人审核</documentation>
      <extensionElements>
        <activiti:formProperty id="pass" name="审核通过" type="boolean" required="true"></activiti:formProperty>
        <activiti:formProperty id="shyj" name="审核意见" type="string"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow7" sourceRef="usertask1" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow8" name="审核未通过" sourceRef="exclusivegateway2" targetRef="endevent3">
      <documentation>审核通过</documentation>
      <extensionElements>
        <activiti:executionListener event="take" expression="${preQualificationNoticeService.updateReviewStatus(execution.processBusinessKey,'2',shyj)}"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!pass}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow9" name="审核通过" sourceRef="exclusivegateway2" targetRef="usertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${pass&&not empty ownerId}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow10" name="审核通过" sourceRef="exclusivegateway6" targetRef="usertask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${pass}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent3" name="审核未通过"></endEvent>
    <sequenceFlow id="flow11" name="审核未通过" sourceRef="exclusivegateway6" targetRef="endevent3">
      <extensionElements>
        <activiti:executionListener event="take" expression="${preQualificationNoticeService.updateReviewStatus(execution.processBusinessKey,'3',shyj)}"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!pass}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway6" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow21" sourceRef="usertask2" targetRef="exclusivegateway6"></sequenceFlow>
    <scriptTask id="scripttask1" name="变量初始化" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>def qualificationNotice=baseService.get("com.mhb.notice.entity.TBPreQualificationNotice",execution.processBusinessKey);
def projectInstance=baseService.get("com.mhb.project.entity.TBProjectInstance",qualificationNotice.getProjectInstanceId());
execution.setVariable("projectInstanceId",projectInstance.getId());
execution.setVariable("projectInstanceName",qualificationNotice.getProjectInstanceName());
execution.setVariable("projectManagerId",projectInstance.getProjectManagerId());
execution.setVariable("ownerId",projectInstance.getOwnerId());
def sdf=new java.text.SimpleDateFormat("yyyy-MM-dd");
def dateStr=sdf.format(qualificationNotice.getPublishDate());
def publishTime=sdf.parse(dateStr);
execution.setVariable("publishTime",publishTime);</script>
    </scriptTask>
    <sequenceFlow id="flow22" sourceRef="startevent1" targetRef="scripttask1"></sequenceFlow>
    <sequenceFlow id="flow23" sourceRef="scripttask1" targetRef="usertask1"></sequenceFlow>
    <sequenceFlow id="flow24" sourceRef="exclusivegateway2" targetRef="usertask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${pass&&empty ownerId}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask3" name="交易平台审核" activiti:candidateGroups="1ae9a74b-784e-41e4-9c04-7d7d7585cdda" activiti:formKey="prequalification/noticeReview">
      <documentation>资格预审结果公告审核-交易平台审核</documentation>
      <extensionElements>
        <activiti:formProperty id="pass" name="审核通过" type="boolean"></activiti:formProperty>
        <activiti:formProperty id="shyj" name="审核意见" type="string"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow25" sourceRef="usertask3" targetRef="exclusivegateway7"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway7" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow26" sourceRef="exclusivegateway7" targetRef="endevent3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!pass}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent4" name="End"></endEvent>
    <sequenceFlow id="flow27" sourceRef="servicetask" targetRef="endevent4"></sequenceFlow>
    <serviceTask id="servicetask" name="状态更新" activiti:expression="${preQualificationNoticeService.updateReviewStatus(execution.processBusinessKey,'9')}">
    </serviceTask>
    <sequenceFlow id="flow28" sourceRef="exclusivegateway7" targetRef="servicetask">
    	<conditionExpression xsi:type="tFormalExpression"><![CDATA[${pass}]]></conditionExpression>
    </sequenceFlow>
  </process>
