  <process id="bid_open" name="开标流程" isExecutable="true">
    <serviceTask id="servicetask1" name="开标检查" activiti:expression="${bidOpenCheckService.execute(execution)}" activiti:resultVariableName="checkSuccess"></serviceTask>
    <sequenceFlow id="flow139" sourceRef="exclusivegateway3" targetRef="parallelgateway5">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${checkSuccess}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow140" sourceRef="servicetask1" targetRef="exclusivegateway3"></sequenceFlow>
    <endEvent id="endevent9" name="开标失败">
      <documentation>签到投标人不足3家</documentation>
    </endEvent>
    <sequenceFlow id="flow141" sourceRef="exclusivegateway3" targetRef="endevent9">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!checkSuccess}]]></conditionExpression>
    </sequenceFlow>
    <parallelGateway id="parallelgateway5" name="Parallel Gateway"></parallelGateway>
    <startEvent id="startevent11" name="等待开标"></startEvent>
    <callActivity id="callactivity1" name="标段开标" calledElement="project_item_open">
      <extensionElements>
        <activiti:in source="projectItemId" target="projectItemId"></activiti:in>
        <activiti:in source="projectInstanceId" target="projectInstanceId"></activiti:in>
        <activiti:in source="projectInstanceName" target="projectInstanceName"></activiti:in>
        <activiti:in sourceExpression="${execution.processBusinessKey}" target="expertApplyId"></activiti:in>
        <activiti:in source="expertSignEnd" target="expertSignEnd"></activiti:in>
        <activiti:in source="roleId" target="roleId"></activiti:in>
        <activiti:in source="expertIdSign" target="expertIdSign"></activiti:in>
        <activiti:in source="envelopeTypeOrder" target="envelopeTypeOrder"></activiti:in>
        <activiti:in source="preliminaryReview" target="preliminaryReview"></activiti:in>
      </extensionElements>
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="${projectItemIdList}" activiti:elementVariable="projectItemId"></multiInstanceLoopCharacteristics>
    </callActivity>
    <sequenceFlow id="flow213" sourceRef="parallelgateway5" targetRef="callactivity1"></sequenceFlow>
    <endEvent id="endevent19" name="End"></endEvent>
    <userTask id="usertask33" name="专家签到" activiti:assignee="${userId}" activiti:formKey="sign/expertSign">
      <documentation>${projectInstanceName} 评标专家签到</documentation>
      <extensionElements>
        <activiti:taskListener event="complete" expression="${signService.expertSign(execution.processBusinessKey)}"></activiti:taskListener>
      </extensionElements>
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="${expertIdSign}" activiti:elementVariable="userId"></multiInstanceLoopCharacteristics>
    </userTask>
    <endEvent id="endevent20" name="End"></endEvent>
    <scriptTask id="scripttask1" name="变量初始化" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>def expertApplyId=execution.getVariable("expertApplyId");
def expertApply=baseService.get("com.mhb.expert.entity.TBExpertApply",expertApplyId);
def map = ["expertApplyId":expertApplyId];
baseService.executeByHQL("delete TBSign where expertApplyId=:expertApplyId and userType='1'",map);
baseService.executeByHQL("delete TBReviewResult where expertApplyId=:expertApplyId",map);
baseService.executeByHQL("delete TBTenderAvgItemScore where expertApplyId = :expertApplyId",map);
baseService.executeByHQL("delete TBQualificationFinalResult where expertApplyId = :expertApplyId",map);	
execution.setVariable('expertSignEnd',false);
runtimeService.updateBusinessKey(execution.getProcessInstanceId(), expertApplyId);
/*开标先定义为第一信封*/
def envelopeTypeOrder="first";
/*cacheManager.getCache("expertApplyCache").put(new net.sf.ehcache.Element(execution.processBusinessKey,expertApply));*/
/*chatManagerService.sendSystemMessage(dlrzj0,freeMarkerBean.getFileTemplateResult("message/bidOpenMessage.ftl",execution.variables));*/
      	
      </script>
    </scriptTask>
    <sequenceFlow id="flow223" sourceRef="parallelgateway5" targetRef="usertask33"></sequenceFlow>
    <scriptTask id="scripttask2" name="更新开标状态" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>def map=[expertApplyId:execution.processBusinessKey,bidCloseDate:new java.util.Date()];
baseService.executeByHQL("update TBExpertApply set bidCloseDate=:bidCloseDate where id=:expertApplyId",map);
/*cacheManager.getCache("expertApplyCache").remove(execution.processBusinessKey);*/
map=[projectInstanceName:projectInstanceName,expertApplyId:execution.processBusinessKey];
/*chatManagerService.sendSystemMessage(dlrzj0,freeMarkerBean.getFileTemplateResult('message/candidatePublicity.ftl',map));*/</script>
    </scriptTask>
    <sequenceFlow id="flow224" sourceRef="callactivity1" targetRef="scripttask2"></sequenceFlow>
    <sequenceFlow id="flow229" name="监标人审核专家签到" sourceRef="usertask33" targetRef="signalintermediatecatchevent1">
      <extensionElements>
        <activiti:executionListener event="take" expression="${guardianService.strartGuardianWorkflow(execution.processBusinessKey,'auditExpertSign')}"></activiti:executionListener>
      </extensionElements>
    </sequenceFlow>
    <sequenceFlow id="flow233" sourceRef="scripttask2" targetRef="endevent19"></sequenceFlow>
    <sequenceFlow id="flow238" sourceRef="startevent11" targetRef="scripttask1"></sequenceFlow>
    <sequenceFlow id="flow239" sourceRef="scripttask1" targetRef="servicetask1"></sequenceFlow>
    <intermediateCatchEvent id="signalintermediatecatchevent1" name="SignalCatchEvent">
      <signalEventDefinition signalRef="auditExpertSignSignal"></signalEventDefinition>
    </intermediateCatchEvent>
    <sequenceFlow id="flow240" sourceRef="signalintermediatecatchevent1" targetRef="endevent20"></sequenceFlow>
  </process>
