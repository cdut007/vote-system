  <process id="notice_review" name="招标公告审核流程" isExecutable="true">
    <startEvent id="startevent1" name="公告审核" activiti:initiator="applyUserId"></startEvent>
    <userTask id="usertask1" name="代理机构审核" activiti:assignee="${dlrzj}" activiti:formKey="notice/notice_review">
      <documentation>${projectInstanceName} ${projectItemName} 公告-代理机构审核</documentation>
      <extensionElements>
        <activiti:formProperty id="custom_view_name" name="表单地址" type="string" expression="notice/notice_review" writable="false"></activiti:formProperty>
        <activiti:formProperty id="noticeId" name="公告ID" type="string" expression="${execution.processBusinessKey}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="pass" name="审核通过" type="boolean" required="true"></activiti:formProperty>
        <activiti:formProperty id="shyj" name="审核意见" type="string"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <userTask id="usertask2" name="招标人审核" activiti:assignee="${ownerId}" activiti:formKey="notice/notice_review">
      <documentation>${projectInstanceName} ${projectItemName} 公告-招标人审核</documentation>
      <extensionElements>
        <activiti:formProperty id="noticeId" name="公告ID" type="string" expression="${execution.processBusinessKey}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="pass" name="审核通过" type="boolean" required="true"></activiti:formProperty>
        <activiti:formProperty id="shyj" name="审核意见" type="string"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <endEvent id="endevent1" name="审核通过"></endEvent>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow7" sourceRef="usertask1" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow8" name="审核未通过" sourceRef="exclusivegateway2" targetRef="endevent3">
      <documentation>审核通过</documentation>
      <extensionElements>
        <activiti:executionListener event="take" expression="${tenderNoticeService.updateReviewStatus(execution.processBusinessKey,'2',shyj)}"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!pass}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow9" name="审核通过" sourceRef="exclusivegateway2" targetRef="usertask2">
      <extensionElements>
        <activiti:executionListener event="take" expression="${tenderNoticeService.updateReviewStatus(execution.processBusinessKey,'6',shyj)}"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${pass&&not empty ownerId}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow10" sourceRef="usertask2" targetRef="exclusivegateway3"></sequenceFlow>
    <endEvent id="endevent3" name="审核未通过"></endEvent>
    <sequenceFlow id="flow11" name="审核未通过" sourceRef="exclusivegateway3" targetRef="endevent3">
