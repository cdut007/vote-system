        <activiti:executionListener event="take" expression="${tenderNoticeService.updateReviewStatus(execution.processBusinessKey,'9')}"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${pass}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask3" name="更新标段状态" activiti:expression="${tenderNoticeService.updateTenderNoticeDataInProjectItem(execution.processBusinessKey)}"></serviceTask>
    <sequenceFlow id="flow36" sourceRef="servicetask3" targetRef="endevent1"></sequenceFlow>
