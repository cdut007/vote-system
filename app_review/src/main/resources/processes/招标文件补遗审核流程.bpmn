    <scriptTask id="scripttask1" name="变量初始化" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>def recruitFileAddendum=baseService.get("com.mhb.document.entity.TBRecruitFileAddendum",execution.processBusinessKey);
def recruitFile=baseService.get("com.mhb.document.entity.TBRecruitFile",recruitFileAddendum.recruitFileId);
def projectInstance=baseService.get("com.mhb.project.entity.TBProjectInstance",recruitFile.projectInstanceId);
def projectItem=baseService.get("com.mhb.project.entity.TBProjectItem",recruitFile.projectItemId);
execution.setVariable("projectManagerId",projectInstance.getProjectManagerId());
execution.setVariable("dlrzj",userService.getPrincipalIdByOrganId(projectInstance.getOrganId()));
execution.setVariable("ownerId",projectInstance.getOwnerId());
execution.setVariable("title",recruitFileAddendum.getTitle());</script>
    </scriptTask>
    <sequenceFlow id="flow29" sourceRef="startevent1" targetRef="scripttask1"></sequenceFlow>
