    <endEvent id="endevent4" name="审核未通过"></endEvent>
    <sequenceFlow id="flow41" name="审核通过" sourceRef="exclusivegateway3" targetRef="endevent4">
      <extensionElements>
        <activiti:executionListener event="take" expression="${recruitFileService.startTenderConfirm(execution.processBusinessKey)}"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${pass}]]></conditionExpression>
    </sequenceFlow>
