  <process id="win_bid_notice_review" name="中标公告审核流程" isExecutable="true">
    <extensionElements>
      <activiti:executionListener event="end" expression="${winBidNoticeReviewEndListener.execute(execution)}"></activiti:executionListener>
    </extensionElements>
    <startEvent id="startevent1" name="中标候选人公示审核" activiti:initiator="applyUserId"></startEvent>
    <userTask id="usertask1" name="项目经理审核" activiti:assignee="${projectManagerId}" activiti:formKey="notice/winBidNoticeReview_pm">
      <documentation>中标公告审核-项目经理审核</documentation>
      <extensionElements>
        <activiti:formProperty id="pass" name="审核通过" type="boolean" required="true"></activiti:formProperty>
        <activiti:formProperty id="shyj" name="审核意见" type="string"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <userTask id="usertask2" name="招标人审核" activiti:assignee="${ownerId}" activiti:formKey="notice/winBidNoticeReview">
      <documentation>中标公告审核-招标人审核</documentation>
      <extensionElements>
        <activiti:formProperty id="pass" name="审核通过" type="boolean" required="true"></activiti:formProperty>
        <activiti:formProperty id="shyj" name="审核意见" type="string"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <endEvent id="endevent1" name="审核通过"></endEvent>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow7" sourceRef="usertask1" targetRef="exclusivegateway8"></sequenceFlow>
    <sequenceFlow id="flow8" name="审核未通过" sourceRef="exclusivegateway2" targetRef="endevent3">
      <documentation>审核通过</documentation>
      <extensionElements>
        <activiti:executionListener event="take" expression="${winBidNoticeService.updateReviewStatus(execution.processBusinessKey,'2',shyj)}"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!pass}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow9" name="审核通过" sourceRef="exclusivegateway2" targetRef="usertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${pass&&not empty ownerId}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow10" name="审核通过" sourceRef="exclusivegateway6" targetRef="usertask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${pass}]]></conditionExpression>
